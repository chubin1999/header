<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <!-- <referenceBlock name="top.search" remove="true"/> -->
        <referenceBlock name="store_language" remove="true" />
        <referenceBlock name="register-link" remove="true" />  
        <referenceBlock name="wish-list-link" remove="true" />
        <referenceBlock name="authorization-link" remove="true" />
        <referenceBlock name="advanced-search-link" remove="true"/>
        <referenceBlock name="header" remove="true"/>
        <referenceBlock name="search-term-popular-link" remove="true" />
        <referenceBlock name="report.bugs" remove="true" />
        <referenceBlock name="catalog-search-advanced-link" remove="true" />
        <referenceBlock name="sales-guest-form-link" remove="true" />
        <!-- <referenceBlock name="form.subscribe" remove="true" /> -->
        <referenceBlock name="report.bugs" remove="true" />
        <referenceBlock name="privacy-policy-link" remove="true" />
        <referenceBlock name="contact-us-link" remove="true" />
        
        <referenceContainer name="header.panel">
            <block class="Magento\Framework\View\Element\Template" before="-" name="header_top" template="Magento_Theme::header_top.phtml" />
        </referenceContainer>
        <referenceContainer name="header-wrapper">
                <container name="header.content.center" htmlTag="div" htmlClass="header-content-top-center">
                    <!-- <block class="Magento\Framework\View\Element\Template" name="test1" template="Magento_Theme::test.phtml" /> -->
                </container>
                
                <container name="header.content.right" htmlTag="div" htmlClass="header-content-right" after="header.content.center">
                    <block class="Magento\Framework\View\Element\Template" after="header.content.center" name="header_right" template="Magento_Theme::header_content_right.phtml" />
                </container>
                <container name="header-bottom" htmlTag="div" htmlClass="header-bot">
                    <block class="Magento\Framework\View\Element\Template" name="nav_bar" template="Magento_Theme::navigation_bar.phtml" />
                </container>
                
        </referenceContainer>
        <referenceContainer name="content">
            <container name="homeblock" htmlTag="div" htmlClass="home-block" after="header-wrapper">
                    <block class="Magento\Framework\View\Element\Template" name="test1" template="Magento_Theme::test.phtml" />
            </container>
            <container name="homeblock.4" htmlTag="div" htmlClass="home-block-4" before="homeblock.5">
                <container name="carousel.homepage" htmlTag="div" htmlClass="carousel-homepage">
                    <container name="carousel.homepage.title" htmlTag="div" htmlClass="carousel-homepage-title">
                        <block class="Magento\Cms\Block\Block" name="raww-favorite">
                            <arguments>
                                <argument name="block_id" xsi:type="string">instagram-title</argument>
                            </arguments>
                        </block>
                    </container>
                    <container name="carousel.homepage.content" htmlTag="div" htmlClass="carousel-homepage-content">
                        <block class="Magento\Framework\View\Element\Template" name="carousel-product" template="Magento_Theme::carousel_homepage.phtml" />
                    </container>
                    
                </container>
            </container> 
            <container name="homeblock.5" htmlTag="div" htmlClass="home-block-5" before="homeblock.6">
                <block class="Magento\Cms\Block\Block" name="instagram-block">
                    <arguments>
                        <argument name="block_id" xsi:type="string">instagram-title</argument>
                    </arguments>
                </block>

                <container name="shop.instagram" htmlTag="div" htmlClass="shop-instagram">
                    <container name="row.1" htmlTag="div" htmlClass="row">
                        <container name="ins.img.1" htmlTag="div" htmlClass="ins-img">
                            <block class="Magento\Cms\Block\Block" name="instagram-img-1">
                                <arguments>
                                    <argument name="block_id" xsi:type="string">shop_img_1</argument>
                                </arguments>
                            </block>
                        </container>
                        <container name="ins.img.2" htmlTag="div" htmlClass="ins-img">
                            <block class="Magento\Cms\Block\Block" name="instagram-img-2">
                                <arguments>
                                    <argument name="block_id" xsi:type="string">shop_img_2</argument>
                                </arguments>
                            </block>
                        </container>
                        <container name="ins.img.3" htmlTag="div" htmlClass="ins-img">
                            <block class="Magento\Cms\Block\Block" name="instagram-img-3">
                                <arguments>
                                    <argument name="block_id" xsi:type="string">shop_img_3</argument>
                                </arguments>
                            </block>
                        </container>
                        <container name="ins.img.4" htmlTag="div" htmlClass="ins-img">
                            <block class="Magento\Cms\Block\Block" name="instagram-img-4">
                                <arguments>
                                    <argument name="block_id" xsi:type="string">shop_img_4</argument>
                                </arguments>
                            </block>
                        </container>
                    </container>

                    <container name="row.2" htmlTag="div" htmlClass="row">
                        <container name="ins.img.5" htmlTag="div" htmlClass="ins-img">
                            <block class="Magento\Cms\Block\Block" name="instagram-img-5">
                                <arguments>
                                    <argument name="block_id" xsi:type="string">shop_img_5</argument>
                                </arguments>
                            </block>
                        </container>
                        <container name="ins.img.6" htmlTag="div" htmlClass="ins-img">
                            <block class="Magento\Cms\Block\Block" name="instagram-img-6">
                                <arguments>
                                    <argument name="block_id" xsi:type="string">shop_img_6</argument>
                                </arguments>
                            </block>
                        </container>
                        <container name="ins.img.7" htmlTag="div" htmlClass="ins-img">
                            <block class="Magento\Cms\Block\Block" name="instagram-img-7">
                                <arguments>
                                    <argument name="block_id" xsi:type="string">shop_img_7</argument>
                                </arguments>
                            </block>
                        </container>
                        <container name="ins.img.8" htmlTag="div" htmlClass="ins-img">
                            <block class="Magento\Cms\Block\Block" name="instagram-img-8">
                                <arguments>
                                    <argument name="block_id" xsi:type="string">shop_img_8</argument>
                                </arguments>
                            </block>
                        </container>
                    </container>
                </container>
            </container>       
            <container name="homeblock.6" htmlTag="div" htmlClass="home-block-6" before="footer">
                <container name="newsletter.block" htmlTag="div" htmlClass="newsletter-block">
                    <block class="Magento\Cms\Block\Block" name="newsletter.title">
                        <arguments>
                            <argument name="block_id" xsi:type="string">newsletter_title</argument>
                        </arguments>
                    </block>
                </container>
            </container>
        </referenceContainer>
        <referenceContainer name="footer">
            <container name="footer.middle" htmlTag="div" htmlClass="footer-middle" before="-">
                <container name="footer.products" htmlTag="div" htmlClass="footer-products" before="-">
                    <block class="Magento\Cms\Block\Block" name="footer.products.content">
                        <arguments>
                            <argument name="block_id" xsi:type="string">footer_products</argument>
                        </arguments>
                    </block>
                </container>
                <container name="footer.our.difference" htmlTag="div" htmlClass="footer-our-difference" after="footer.products">
                    <block class="Magento\Cms\Block\Block" name="footer.our.difference.content">
                        <arguments>
                            <argument name="block_id" xsi:type="string">footer_our_difference</argument>
                        </arguments>
                    </block>
                </container>
                <container name="footer.customer.service" htmlTag="div" htmlClass="footer-customer-service" after="footer.our.difference">
                    <block class="Magento\Cms\Block\Block" name="footer.customer.service.content">
                        <arguments>
                            <argument name="block_id" xsi:type="string">footer_customer_service</argument>
                        </arguments>
                    </block>
                </container>
                <container name="footer.social" htmlTag="div" htmlClass="footer-social" after="footer.customer.service">
                    <block class="Magento\Cms\Block\Block" name="footer.social.icon">
                        <arguments>
                            <argument name="block_id" xsi:type="string">social_icon</argument>
                        </arguments>
                    </block>
                </container>
            </container>
            <container name="footer.middle" htmlTag="div" htmlClass="footer-middle mobile" before="-">
                <container name="footer.products" htmlTag="div" htmlClass="footer-products col" before="-">
                    <block class="Magento\Cms\Block\Block" name="footer.products.content">
                        <arguments>
                            <argument name="block_id" xsi:type="string">footer_products</argument>
                        </arguments>
                    </block>
                </container>
                <container name="footer.our.difference" htmlTag="div" htmlClass="footer-our-difference col" after="footer.products">
                    <block class="Magento\Cms\Block\Block" name="footer.our.difference.content">
                        <arguments>
                            <argument name="block_id" xsi:type="string">footer_our_difference</argument>
                        </arguments>
                    </block>
                </container>
                <container name="footer.customer.service" htmlTag="div" htmlClass="footer-customer-service col" after="footer.our.difference">
                    <block class="Magento\Cms\Block\Block" name="footer.customer.service.content">
                        <arguments>
                            <argument name="block_id" xsi:type="string">footer_customer_service</argument>
                        </arguments>
                    </block>
                </container>
                <container name="footer.social" htmlTag="div" htmlClass="footer-social col" after="footer.customer.service">
                    <block class="Magento\Cms\Block\Block" name="footer.social.icon">
                        <arguments>
                            <argument name="block_id" xsi:type="string">social_icon</argument>
                        </arguments>
                    </block>
                </container>
            </container>
            
        </referenceContainer>
        <referenceContainer name="footer-container">
            <container name="footer.top" htmlTag="div" htmlClass="footer-top" before="footer">
               
                <block class="Magento\Cms\Block\Block" name="footer.top.content">
                    <arguments>
                        <argument name="block_id" xsi:type="string">footer_top_content</argument>
                    </arguments>
                </block>

            </container>
            <container name="footer.bottom" htmlTag="div" htmlClass="footer-bottom" after="footer">
                <container name="footer.bottom.content" htmlTag="div" htmlClass="footer-bottom-content" after="footer">
                    <container name="footer.bottom.copyright" htmlTag="div" htmlClass="footer-bottom-copyright">
                    </container>
                    <container name="footer.bottom.payment" htmlTag="div" htmlClass="footer-bottom-payment" after="footer.bottom.copyright">
                        <block class="Magento\Cms\Block\Block" name="footer.payment">
                            <arguments>
                                <argument name="block_id" xsi:type="string">footer_payment</argument>
                            </arguments>
                        </block>
                    </container>
                </container>
            </container>
        </referenceContainer>
        <!-- <referenceContainer name="header.panel">
            <block name="nav.toggle" before="-">
                <arguments>
                    <argument name="block_id" xsi:type="string">footer_payment</argument>
                </arguments>
            </block>
        </referenceContainer> -->

        <move element="minicart" destination="header.content.right" after="header_right" />
        <move element="top.search" destination="header.content.center" before="test1"/>
        <move element="header.content.right" destination="header-wrapper" after="header.content.center"/>
        <move element="store_language" destination="footer_links"/>
        <move element="header-bottom" destination="page.wrapper" before="main.content" />
        <move element="copyright" destination="footer.bottom.copyright" />
        <move element="contact-us-link" destination="footer.customer.service" />
        <move element="form.subscribe" destination="homeblock.6" />
        <!-- <move element="newsletter.title" destination="form.subscribe" /> -->

    </body>
</page>